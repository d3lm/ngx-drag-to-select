{
  "$schema": "../../node_modules/ng-packagr/package.schema.json",
  "name": "ngx-drag-to-select",
  "version": "1.1.1",
  "description": "A lightweight, fast, configurable and reactive drag-to-select component for Angular 5 and beyond",
  "sideEffects": false,
  "peerDependencies": {
    "@angular/core": ">=6.0.0",
    "@angular/common": ">=6.0.0",
    "rxjs": ">=6.1.0"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts",
      "languageLevel": [
        "es2017",
        "dom"
      ],
      "comments": "none"
    },
    "dest": "../../dist/lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d3lm/ngx-drag-to-select.git"
  },
  "keywords": [
    "angular",
    "ng2",
    "ngx",
    "ng",
    "typescript",
    "angular2",
    "select",
    "drag"
  ],
  "author": "Dominic Elm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d3lm/ngx-drag-to-select/issues"
  },
  "homepage": "https://github.com/d3lm/ngx-drag-to-select#readme"
}
